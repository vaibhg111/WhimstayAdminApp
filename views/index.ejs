    <script>


        $("#dob_year,#dob_month,#dob_day").change(function () {
            getStartDate();
        });
        function getStartDate() {
            var startAth = $("#dob_year").val();
            if (startAth === "") {
                $("#fullDob").val('');
                // console.log("start at hours missing");
                return;
            }
            var startAtm = $("#dob_month").val();
            if (startAtm == "") {
                $("#fullDob").val('');
                //   console.log("start at minutes missing");
                return;
            }
            var startAtam = $("#dob_day").val();
            if (startAtm == "") {
                $("#fullDob").val('');
                //  console.log("start at AM/PM missing");
                return;
            }
            var startDate = startAth + "-" + startAtm + "-" + startAtam;
            $("#fullDob").val(startDate);
        }

    </script>
